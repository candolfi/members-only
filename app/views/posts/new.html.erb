<h1>New Article</h1>
<%= form_with model: @post do |form| %>
    <div>
        <%= form.label :title %>
        <%= form.text_field :title %><br>
        <%= form.label :content %>
        <%= form.text_area :content %><br>
    </div>
    <div>
        <%= form.submit "Submit"%>
    </div>

<% end %>

<%= button_to "Sign Out", destroy_user_session_path, method: :delete %>



